<%= form_tag("/passes", :method=>"post") do %>

	<%= label_tag :description, 'Description:' %>
	<%= text_field_tag(:description) %>
	
	<%= label_tag :logo, 'Logo:' %>
	<%= text_field_tag(:logoText) %>

	<%= hidden_field_tag :serialNumber %>
	<%= hidden_field_tag :formatVersion %>
	<%= hidden_field_tag :passTypeIdentifier %> 
	<%= hidden_field_tag :webServiceURL %>
	<%= hidden_field_tag :authentificationToken %>
	<%= hidden_field_tag :teamIdentifier %>
	<%= hidden_field_tag :barcode %>

	<%= submit_tag("Generate Pass", :class=>"btn") %>
<% end %>
